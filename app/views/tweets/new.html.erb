<h1> New Tweet </h1>
<%= form_with scope: :tweet,url: tweets_path, local: true do |form| %>
  <p>
    <%= form.label :user_id %><br>
    <%= form.text_field :user_id %>
  </p>
 
  <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>
<%= link_to 'Back', tweets_path %>