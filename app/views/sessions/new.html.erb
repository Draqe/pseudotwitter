<%= form_for(:session, url: sessions_path) do |f| %>
  <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
  <div class="form-group">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="form-group">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <%= f.submit 'Log In', class: "btn btn-primary btn-lg"%>
<% end %>
